{
    "name": "orkhanahmadov/cbar-currency",
    "description": "PHP library to work with cbar.az currency rates",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-simplexml": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "blastcloud/guzzler": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\CBARCurrency\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\CBARCurrency\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
